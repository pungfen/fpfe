{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {

    "target": "ESNext",

    "jsx": "preserve",
    "moduleDetection": "force",
    "module": "ESNext",
    "moduleResolution": "bundler",

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["*.config.ts", "src"]
}
