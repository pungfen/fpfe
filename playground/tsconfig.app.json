{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "types": [
      "vite/client",
      "unplugin-vue-router/client",
      "unplugin-icons/types/vue",
      "@fpfe/ui/types"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "auto-imports.d.ts",
    "components.d.ts",
    "typed-router.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ]
}

